{
    "name": "mavv3006/blue-bird-big-band",
    "type": "project",
    "description": "Fullstack Laravel+Vue.js project for the website of the Blue Bird Big Band from the music school of Speyer, Germany.",
    "homepage": "https://www.bluebirdbigband.de",
    "authors": [
        {
            "name": "Marvin Deuschle",
            "role": "Maintainer"
        }
    ],
    "support": {
        "email": "bbbb-maintainers@googlegroups.com",
        "issues": "https://github.com/Mavv3006/BlueBirdBigBand/issues",
        "source": "https://github.com/Mavv3006/BlueBirdBigBand"
    },
    "require": {
        "php": "^8.3",
        "ext-pdo": "*",
        "althinect/filament-spatie-roles-permissions": "3.x-dev",
        "filament/filament": "4.0",
        "guzzlehttp/guzzle": "^7.5.2",
        "inertiajs/inertia-laravel": "^2.0",
        "johngrogg/ics-parser": "^3.4",
        "laravel/framework": "^12.10",
        "laravel/sanctum": "^4.0",
        "laravel/tinker": "^2.7",
        "livewire/livewire": "^3.0",
        "spatie/laravel-permission": "^6.7",
        "tightenco/ziggy": "^v2.1.0"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.9",
        "fakerphp/faker": "^1.22.0",
        "filament/upgrade": "4.0",
        "friendsofphp/php-cs-fixer": "^v3.17.0",
        "larastan/larastan": "^3",
        "laravel/breeze": "^2.0",
        "laravel/pint": "^v1.10.0",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^8.1",
        "phpunit/phpunit": "^12.3.5",
        "spatie/laravel-ignition": "^2.1.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi",
            "@php artisan filament:upgrade"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\"",
            "@php -r \"fopen('database/database.sqlite', 'w');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "php-cs-fixer": "php-cs-fixer",
        "dev": [
            "Composer\\Config::disableProcessTimeout",
            "npx concurrently -c \"#93c5fd,#c4b5fd,#fb7185\" \"php artisan serve\" \"npm run dev\" \"docker compose up\" --names=server,vite,database --kill-others"
        ],
        "phpstan": "./vendor/bin/phpstan analyse -c ./phpstan.neon",
        "test": "php artisan test --coverage"
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
